  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/95E6-B20B";
    fsType = "vfat";
    options = [
      "fmask=0077"
      "dmask=0077"
    ];
  };

  swapDevices = [
    { device = "/dev/disk/by-uuid/9b2ef379-0045-4948-a52a-c89370558a95"; }
