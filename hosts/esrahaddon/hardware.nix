  fileSystems."/" = {
    device = "/dev/disk/by-uuid/b9157699-911b-46e0-8df9-8c451bef157c";
    fsType = "ext4";
  };

  boot.initrd.luks.devices."luks-dcbe35da-acd5-4cdd-becc-2d2171fd7ab7".device =
